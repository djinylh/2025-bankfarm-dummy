<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "com.bankfarm_dummy.bankfarm_dummy.card.CardOptionParamMapper">


    <insert id="cardOptionParamDetail">

        INSERT INTO card_option_param
        SET card_option_def_id = #{cardOptionCode}
        ,card_option_des = #{cardOptionName}
        , card_active_yn = #{cardActiveYn}

    </insert>

<select id="getListByOption">
    SELECT card_option_code
    FROM card_option_def
</select>

</mapper>